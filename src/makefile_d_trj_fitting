TARGET=n_trj_fitting.x

CXX=icpc

CXXFLAGS= -O3 -std=c++11 -I../include -I../include/uiuc

LIBS=

SRCS = dcdplugin.cpp Atom.cpp Index.cpp NAMDBin.cpp PDBUtil.cpp PSF.cpp PDB.cpp ComputeRMSD.cpp n_trj_fitting.cpp

OBJS = $(SRCS:.cpp=.o)

HEADS= common.hpp Atom.hpp Index.hpp ReadDCD.hpp NAMDBin.hpp PDBUtil.hpp PSF.hpp PDB.hpp ComputeRMSD.hpp

$(TARGET): $(OBJS) $(HEADS)
	     $(CXX) $(CXXFLAGS) -o $(TARGET) $(OBJS) $(LIBS)

clean:
	rm -f $(OBJS) $(TARGET)
