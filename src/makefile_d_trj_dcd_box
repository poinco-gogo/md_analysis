TARGET=n_trj_dcd_box.x

CXX=icpc

CXXFLAGS= -O3 -std=c++11 -I../include -I../include/uiuc

LIBS=

SRCS = dcdplugin.cpp Atom.cpp Index.cpp PDBUtil.cpp PSF.cpp PDB.cpp n_trj_dcd_box.cpp

OBJS = $(SRCS:.cpp=.o)

HEADS=

$(TARGET): $(OBJS) $(HEADS)
	     $(CXX) $(CXXFLAGS) -o $(TARGET) $(OBJS) $(LIBS)

clean:
	rm -f $(OBJS) $(TARGET)
