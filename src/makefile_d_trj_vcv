TARGET=d_trj_vcv.x

CXX=icpc

CXXFLAGS= -O3 -std=c++11 -I../include -I../include/uiuc

LIBS=

SRCS = dcdplugin.cpp Atom.cpp Index.cpp NAMDBin.cpp MatrixUtil.cpp PDBUtil.cpp PSF.cpp PDB.cpp d_trj_vcv.cpp

OBJS = $(SRCS:.cpp=.o)

HEADS= common.hpp Atom.hpp Index.hpp ReadDCD.hpp NAMDBin.hpp MatrixUtil.hpp PDBUtil.hpp PSF.hpp PDB.hpp

$(TARGET): $(OBJS) $(HEADS)
	     $(CXX) $(CXXFLAGS) -o $(TARGET) $(OBJS) $(LIBS)

clean:
	rm -f $(OBJS) $(TARGET)
