TARGET=d_show_index_atoms.x

CXX=icpc

CXXFLAGS= -O3 -std=c++11 -I../include -I../include/uiuc

LIBS=

SRCS = Atom.cpp Index.cpp PDBUtil.cpp PSF.cpp PDB.cpp d_show_index_atoms.cpp

OBJS = $(SRCS:.cpp=.o)

HEADS=

$(TARGET): $(OBJS) $(HEADS)
	     $(CXX) $(CXXFLAGS) -o $(TARGET) $(OBJS) $(LIBS)

clean:
	rm -f $(OBJS) $(TARGET)
